<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="https://github.com/dibp/wcomponents/namespace/ui/v1.0"
	xmlns:ui="https://github.com/dibp/wcomponents/namespace/ui/v1.0">

	<xs:element name="hr">
		<xs:annotation>
			<xs:documentation>WHorizontalRule represents a horizontal content separator with no instrinsic semantic meaning. It is best avoided as it provides only a visual
				separation and may have negative accessibility implications.</xs:documentation>
		</xs:annotation>
		<xs:complexType />
	</xs:element>

</xs:schema>
