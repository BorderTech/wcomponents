# EditorConfig: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset=utf-8
end_of_line=lf
insert_final_newline=true
indent_style=tab
indent_size=4

[*.java]
trim_trailing_whitespace=true

[*.js]
trim_trailing_whitespace=true

[*.properties]
trim_trailing_whitespace=false

[*.scss]
indent_style=space
indent_size=2

[{*.yml,*.yaml}]
indent_style=space
indent_size=2
