<dialog id="wc_dlgid" role="dialog">
<header tabindex="0">
<span>
<button class="wc_maxcont wc_btn_icon" type="button" title="{{heading.maxRestore}}" aria-pressed="false" data-wc-resize="wc_dlgid"><i aria-hidden="true" class="fa fa-plus"></i></button>
<button class="wc_dialog_close wc_btn_icon" type="button" title="{{heading.close}}"><i aria-hidden="true" class="fa fa-times"></i></button>
</span>
<h1>&#x2002;</h1>
</header>
<div class="content" aria-live="assertive">
	{{message.loading}}
</div>
<footer>
<button class="wc-nobutton wc_resize" data-wc-resize="wc_dlgid" type="button"><i aria-hidden="true" class="fa fa-arrows-alt"></i></button>
</footer>
</dialog>
