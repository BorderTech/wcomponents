<dialog id="wc_dlgid" role="dialog">
<header tabindex="0">
<span>
<button class="wc_maxcont wc_btn_icon" type="button" title="{{heading.maxRestore}}" aria-pressed="false" data-wc-resize="wc_dlgid"> </button>
<button class="wc_dialog_close wc_btn_icon" type="button" title="{{heading.close}}"> </button>
</span>
<h1>&#x2002;</h1>
</header>
<div class="content" aria-live="assertive">
	{{message.loading}}
</div>
<footer>
<button class="wc-nobutton wc_resize wc-icon" data-wc-resize="wc_dlgid" type="button">&#x2002;</button>
</footer>
</dialog>
