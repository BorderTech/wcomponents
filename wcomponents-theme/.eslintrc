{
	"env": {
		"browser": true,
		"amd": true
	},
	"extends": "eslint:recommended",
	"rules": {
		"no-console": 0,
		"no-shadow": 2,
		"dot-notation": 0,
		"no-underscore-dangle": 0,
		"no-constant-condition": 0,
		"no-return-assign": 0,
		"no-extend-native": 0,
		"no-debugger": 0,
		"no-alert": 0,
		"brace-style": 2,
		"semi": [2, "always"],
		"space-before-blocks": [2, "always"],
		"spaced-comment": [2, "always"],
		"keyword-spacing": 2,
		"indent": [2, "tab", { "SwitchCase": 1 } ],
		"quotes": 0
	},
	"globals": {
		"KeyEvent": true,
		"Promise": true,
		"DataView": true
	}
}
