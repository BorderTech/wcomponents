{
	"name": "wc",
	"version": "1.5.30-SNAPSHOT",
	"description": "Client side code for WComponents UI tool kit.",
	"private": true,
	"type": "module",
	"com_github_bordertech": {
		"description": "Custom config here. JSON has no comment syntax, argh!",
		"testMinOrMax": "max",
		"name": "wcomponents-theme"
	},
	"scripts": {
		"build": "node build-images.mjs && node build-resource.mjs && node build-css.mjs && node build-js.mjs",
		"build:css": "node build-css.mjs",
		"build:img": "node build-images.mjs",
		"build:js": "node build-js.mjs",
		"lint": "node scripts/lintfile.mjs",
		"site": "node node_modules/jsdoc/jsdoc.js -c jsdoc.config.json",
		"test": "jasmine --config=jasmine.json",
		"test:grid": "grunt --gruntfile Gruntfile.mjs test:grid",
		"test:local": "grunt --gruntfile Gruntfile.mjs test:local",
		"test:node": "grunt --gruntfile Gruntfile.mjs intern:node",
		"test:serve": "grunt --gruntfile Gruntfile.mjs test:serve",
		"watch": "node scripts/theme-watch.mjs"
	},
	"directories": {
		"src": "src/main",
		"test": "src/test",
		"target": "target"
	},
	"exports": {
		"./*": "./src/main/js/wc/*"
	},
	"dependencies": {
		"fabric": "3.4.0",
		"i18next": "23.5.1",
		"mailcheck": "1.1.1",
		"requirejs": "2.3.6",
		"sprintf-js": "1.1.3",
		"tinymce": "6.3.1"
	},
	"devDependencies": {
		"@babel/cli": "7.20.7",
		"@babel/core": "7.20.12",
		"@testing-library/dom": "9.3.1",
		"@testing-library/jasmine-dom": "^1.3.3",
		"@types/fs-extra": "11.0.1",
		"@types/jasmine": "^4.3.5",
		"@types/node": "20.6.0",
		"@types/sass": "1.45.0",
		"@types/socket.io": "3.0.2",
		"@types/uglify-js": "3.17.2",
		"axe-core": "3.0.0",
		"babel-plugin-transform-commonjs": "1.1.6",
		"babel-plugin-transform-import-as-amd": "0.0.12",
		"chokidar": "3.5.3",
		"clean-css-cli": "4.1.11",
		"eslint": "8.47.0",
		"fs-extra": "8.1.0",
		"global-jsdom": "^9.1.0",
		"grunt": "1.6.1",
		"grunt-contrib-clean": "2.0.1",
		"grunt-contrib-copy": "1.0.0",
		"i18next-fs-backend": "^2.1.5",
		"intern": "4.10.1",
		"jasmine": "^5.1.0",
		"jsdoc": "3.6.3",
		"jsdom": "^22.1.0",
		"properties": "1.2.1",
		"sass": "1.5.1",
		"sass-lint": "1.12.1",
		"socket.io": "4.7.2",
		"uglify-js": "3.17.4"
	}
}
