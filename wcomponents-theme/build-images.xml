<project name="BuildImages" default="build" >

	<import file="build-import.xml"/>

	<target name="build" description="Builds images">
		<copy todir="${target.rootdir}/images" overwrite="true">
			<fileset dir="${common.src.rootdir}/images" includes="**"/>
		</copy>
		<copy todir="${target.rootdir}/images" overwrite="true">
			<fileset dir="${impl.src.main.dir}/images" includes="**" erroronmissingdir="false"/>
		</copy>
	</target>

</project>
