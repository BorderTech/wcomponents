<FindBugsFilter>
     <!-- exclude unit tests (class ends with "test") -->
     <Match>
        <Class name="~.*\..*[Tt]est" />
     </Match>
     <!-- exclude unit tests (class starts with "Test") -->
     <Match>
        <Class name="~.*\.Test[A-Z].*" />
     </Match>
     <!-- exclude emf generated classes based on the SFP schema (package will contain ".v1." etc.) -->
     <Match>
        <Class name="~.*\.v[1-9][0-9]*\..*" />
     </Match>
	<!-- Exclude other generated classes (will have package containing e.g. "gen") -->
	<Match>     
		<Class name="~.*\.gen\..*" />
	</Match>
</FindBugsFilter>